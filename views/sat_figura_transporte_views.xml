    <record id="action_sat_figura_transporte" model="ir.actions.act_window">
        <field name="name">Figuras de Transporte SAT</field>
        <field name="res_model">tms.sat.figura.transporte</field>
        <field name="view_mode">list,form</field>
    </record>
