<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sat_tipo_permiso_tree" model="ir.ui.view">
        <field name="name">tms.sat.tipo.permiso.tree</field>
        <field name="model">tms.sat.tipo.permiso</field>
        <field name="arch" type="xml">
            <list string="Tipos de Permiso SCT">
                <field name="code"/>
                <field name="name"/>
                <field name="clave_transporte"/>
