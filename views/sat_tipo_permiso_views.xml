        </field>
    </record>

    <record id="action_sat_tipo_permiso" model="ir.actions.act_window">
        <field name="name">Tipos de Permiso SCT</field>
        <field name="res_model">tms.sat.tipo.permiso</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>

