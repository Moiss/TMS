<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sat_localidad_tree" model="ir.ui.view">
        <field name="name">tms.sat.localidad.tree</field>
        <field name="model">tms.sat.localidad</field>
        <field name="arch" type="xml">
            <list string="Localidades SAT">
                <field name="code"/>
                <field name="name"/>
                <field name="estado"/>
            </list>
        </field>
    </record>

    <record id="action_sat_localidad" model="ir.actions.act_window">
        <field name="name">Localidades SAT</field>
        <field name="res_model">tms.sat.localidad</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>

